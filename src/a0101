public class Klausurergebnisse{

       public static void main(String[]args){
            double note = 1.0;
            int zahl = (int) note;
            switch(zahl){
                case 1: // beabsichtigter fall-through
                case 2:
                case 3:
                case 4:
                    System.out.println(„Klausur mit Note “ + zahl + „ bestanden“);
                    break;
                case 5:
                    System.out.println(„Nicht bestanden“);
      }
   }
}
